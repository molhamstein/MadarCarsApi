{
  "name": "invoice",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "replaceOnPUT": false,
  "scope": {
    "include": [
      {
        "relation": "invoiceType"
      }
    ]
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "cost": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "invoiceType": {
      "type": "belongsTo",
      "model": "invoiceType",
      "foreignKey": "invoiceTypeId",
      "options": {
        "nestRemoting": true
      }
    },
    "innerInvoice": {
      "type": "belongsTo",
      "model": "innerInvoice",
      "foreignKey": "innerId",
      "options": {
        "nestRemoting": true
      }
    },
    "outerInvoice": {
      "type": "belongsTo",
      "model": "outerInvoice",
      "foreignKey": "outerId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
