{
  "name": "outerBill",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string",
      "required": true,
      "default": "notBilled"
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "bills": {
      "type": "hasMany",
      "model": "bill",
      "foreignKey": "outerBillId",
      "options": {
        "nestRemoting": true
      }
    },
    "trip": {
      "type": "belongsTo",
      "model": "trip",
      "foreignKey": "tripId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
